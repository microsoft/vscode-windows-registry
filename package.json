{
  "name": "@vscode/windows-registry",
  "version": "1.0.6",
  "description": "A native node module for accessing the windows registry",
  "os": [
    "win32"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
	"dist/index.js",
	"dist/index.d.ts",
	"dist/winregistry.node"
  ],
  "scripts": {
	"prepublishOnly": "npm run compile && npm run test",
	"compile": "[ -f binding.gyp ] && tsc -p ./ && node-gyp rebuild && cp build/Release/winregistry.node dist/",
    "test": "[ -d test ] && mocha -r ts-node/register test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/vscode-windows-registry.git"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-windows-registry/issues"
  },
  "homepage": "https://github.com/Microsoft/vscode-windows-registry#readme",
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^14",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
